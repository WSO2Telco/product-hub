                                  ......

            -->
            <throttle:ID throttle:type="ROLE">Subscription</throttle:ID>
            <wsp:Policy>
                <throttle:Control>
                    <wsp:Policy>
                        <throttle:MaximumCount>20</throttle:MaximumCount>
                        <throttle:UnitTime>60000</throttle:UnitTime>
                        <wsp:Policy>
                            <throttle:Attributes>
                                <throttle:Rate>10</throttle:Rate>
                                <throttle:Description>
                                    <![CDATA[<div><h4>Gold Tier</h4><br/><br/><strong>Throttling Info</strong><br/>20 requests per minute<br/><br/><strong>Subscription</strong><br/> 10 per request</div>]]>
